config_version=5

[application]

config/name="Game"
config/features=PackedStringArray("4.0", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

RDS="*res://Scripts/ResourceRelated/ResourceDataServer.gd"
FS="*res://Scripts/BuildingRelated/FacilityServer.gd"
NS="*res://Scripts/NaviRelated/NaviServer.gd"

[editor]

export/convert_text_resources_to_binary=true

[rendering]

renderer/rendering_method="gl_compatibility"
